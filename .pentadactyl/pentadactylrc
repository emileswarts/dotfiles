" use the listed characters to enumerate quickhint elements
set hintkeys=arstdhneiofu

" toggle menu toolbar
nmap <C-F1> :set guioptions!=m<CR>
" toggle bookmarks toolbar
nmap <C-F2> :set guioptions!=B<CR>

" view native size of image without clicking it
" nmap <leader>z :js window.content.document.toggleImageSize();<CR>

nmap ee gt
nmap en gT
colorscheme cf8

" FASTER PLEASE
nmap -builtin j 4j
nmap -builtin k 4k

nmap -builtin <c-f> <f12>

" Shift is hard.
nmap -builtin ; :

set hlfind

nmap -builtin ,<space> <esc>:noh<CR>
